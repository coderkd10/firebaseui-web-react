{
    "presets": [
        "@babel/preset-flow",
        "@babel/preset-react",
        ["@babel/preset-env", {
            "targets": { "node": "4.1" },
            "loose": true,
            "modules": "commonjs"
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-class-properties", {
            "loose": true 
        }],
        "@babel/plugin-transform-object-rest-spread"
    ],
    "comments": false
}
